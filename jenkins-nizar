                echo 'building stage succesfuly'
            }
        }
        stage('Test') { 
            steps {
                echo 'Testing stage done..'
            }
        }
        stage('Deploy') { 
            steps {
                echo 'Deploying stage done ...'
            }
        }
    }
}
